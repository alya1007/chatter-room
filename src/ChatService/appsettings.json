{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"Kestrel": {
		"Endpoints": {
			"Http1ForWebSockets": {
				"Url": "http://*:5008",
				"Protocols": "Http1"
			},
			"Http2ForGrpc": {
				"Url": "http://*:5001",
				"Protocols": "Http2"
			}
		},
		"EndpointDefaults": {
			"Protocols": "Http1AndHttp2"
		},
		"WebSockets": {
			"KeepAliveInterval": "00:20:00",
			"BufferSize": 4096
		}
	}
}
